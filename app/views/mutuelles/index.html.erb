<p id="notice"><%= notice %></p>

<h1>Listing Mutuelles</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mutuelles.each do |mutuelle| %>
      <tr>
        <td><%= link_to 'Show', mutuelle %></td>
        <td><%= link_to 'Edit', edit_mutuelle_path(mutuelle) %></td>
        <td><%= link_to 'Destroy', mutuelle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mutuelle', new_mutuelle_path %>
